
public class C22 {

	public static void main(String[] args) {

		int i = 3;
		
		while (i < 9) {
			
		 int j = 1; 
		 if(i ==3 || i == 5) {
			 
			 System.out.println(i+"ë‹¨");
		 }
		
		 
		 while (j < 10) {
			System.out.println(i+"X"+j+"="+i*j);
			j++;
			 
		 }
		
		i++;
			
		
		}
		
		
}
}
