import java.util.Scanner;

public class Baeyul3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] aa = new int[4];
		int hap = 0, i;

		for (i = 0; i < 4; i++) {
			System.out.println(i + " 번째 숫자를 입력하세요 ");

			aa[i] = sc.nextInt();

		}

		
		hap = aa[0] + aa[1] + aa[2] + aa[3];
		
		System.out.println("합계 ==> "+ hap);
	}

}
