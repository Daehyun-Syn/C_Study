package assignment;

public class OperationEx {

	public static void main(String[] args) {

		 int age = 24;
		 System.out.println(age);
		 // 대입 연산자는 우선순위가 가장 낮음
		 
		 int num = 10;
		 int num2 = -num;
		System.out.println(num);
		System.out.println(num2);
		
num = -num;
System.out.println(num);
//변수의 값을 바꾸려면 대입연산자 사용하여 바꿈

/*산술연산자 % : 나머지를 구하는 연산자 
 * 앞에 있는 항에서 뒤에있는 항을 나누어 나머지를 구한다*/
//숫자 n의 나머지는 0~n-1 범위의 수
//특정 범위 안의 수를 구할때 종종 사용한다


	 
		 }
	

}
